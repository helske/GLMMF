} else return Rcpp::List::create(Rcpp::Named(\"convergence\") = conv);\n  \n  return Rcpp::List::create(Rcpp::Named(\"linear.predictor\") = theta, Rcpp::Named(\"coefs\") = coefs,\n  Rcpp::Named(\"coefVars\") = coefVars,\n  Rcpp::Named(\"logLik\") = loglik,Rcpp::Named(\"convergence\") = conv);\n}\n",
